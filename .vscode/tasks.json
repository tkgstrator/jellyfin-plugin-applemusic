{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run act (build.yaml)",
      "type": "shell",
      "command": "act",
      "args": [
        "-W",
        "${workspaceFolder}/.github/workflows/build.yaml",
        "--artifact-server-path",
        "artifacts"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}
